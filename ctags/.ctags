--recurse=yes
--tag-relative=yes
--exclude=.git
--exclude=*bower_components/*
--exclude=*node_modules/*

--langdef=typescript
--langmap=typescript:.ts
--langmap=typescript:.d.ts
--regex-typescript=/^ *(export )? *var +([a-zA-Z0-9_]+)/\2/v,variables/
--regex-typescript=/^ *(export )? *function +([a-zA-Z0-9_]+)/\2/f,functions/
--regex-typescript=/^ *(export )? *module +([a-zA-Z0-9_]+)/\2/n,modules/
--regex-typescript=/^ *(export )? *interface \t]+([a-zA-Z0-9_]+)/\2/i,interfaces/
--regex-typescript=/^ *(export )? *class +([a-zA-Z0-9_]+)/\2/c,classes/
--regex-typescript=/(public |private ) *([a-zA-Z0-9_]+)/\2/p,membervars/
--regex-typescript=/^ *(public |private ) *([a-zA-Z0-9_]+)/\2/m,methods/
--regex-typescript=/^ *(export)? *enum +([a-zA-Z0-9_]+)/\2/e,enums/
